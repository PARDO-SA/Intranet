@model IEnumerable<Intranet.Models.Clases.Consultas.ViewComisiones>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 align="center">Resumen de Comisiones de BlisterPack</h2>

<div class="cuerpo tablaPAR-Consultas">
    <table id="tablaComisiones" class="cell-border display compact" cellspacing="0">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Sucursal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Nombre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Funcion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cobrado)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ComisionVen)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ComisionSuc)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ACobrar)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RestaCobrar)
                </th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Sucursal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Funcion)
                </td>
                <td align="right">
                    @Html.DisplayFor(modelItem => item.Cobrado)
                </td>
                <td align="right">
                    @Html.DisplayFor(modelItem => item.ComisionVen)
                </td>
                <td align="right">
                    @Html.DisplayFor(modelItem => item.ComisionSuc)
                </td>
                <td align="right">
                    @Html.DisplayFor(modelItem => item.ACobrar)
                </td>
                <td align="right">
                    @Html.DisplayFor(modelItem => item.RestaCobrar)
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>